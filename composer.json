{
    "name": "schams-net/pagetitle",
    "type": "typo3-cms-extension",
    "description": "Demo extension to showcase TYPO3's PageTitle API",
    "authors": [
        {
            "name": "Michael Schams",
            "role": "Developer"
        }
    ],
    "require": {
        "typo3/cms-core": ">=9.5.0"
    },
    "autoload": {
        "psr-4": {
            "SchamsNet\\Pagetitle\\": "Classes"
        }
    },
    "replace": {
        "pagetitle": "self.version",
        "schams-net/pagetitle": "self.version"
    }
}
